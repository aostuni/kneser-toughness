Cell["\<\
Similarly try for K(10, 4).
Runs slowly, so use binary search.
Returns the first S for which we cannot get a contradiction.\
\>", "Text",
 CellChangeTimes->{{3.7993367216557884`*^9, 3.799336730893914*^9}, {
  3.7993376016691055`*^9, 3.799337608122797*^9}, {3.7993391708226967`*^9, 
  3.799339179050935*^9}},ExpressionUUID->"fb5c5ace-e7b8-4776-975e-\
e410d7b58548"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"g", "[", "c_", "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"lo", "=", 
       RowBox[{"c", "+", "1"}]}], ",", 
      RowBox[{"hi", "=", 
       RowBox[{"Ceiling", "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           FractionBox["10", "4"], "-", "1"}], ")"}], "c"}], "]"}]}], ",", 
      RowBox[{"m", "=", "0"}]}], "}"}], ",", 
    RowBox[{
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"lo", "<", "hi"}], ",", 
       RowBox[{
        RowBox[{"m", "=", 
         RowBox[{"Floor", "[", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"lo", "+", "hi"}], ")"}], "/", "2"}], "]"}]}], ";", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"l", "[", 
            RowBox[{"10", ",", "4", ",", "c", ",", "m"}], "]"}], ">", 
           RowBox[{"u", "[", 
            RowBox[{"10", ",", "4", ",", "c", ",", "m"}], "]"}]}], ",", 
          RowBox[{"lo", "=", 
           RowBox[{"m", "+", "1"}]}], ",", 
          RowBox[{"hi", "=", "m"}]}], "]"}]}]}], "]"}], ";", "lo"}]}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{"g", "[", "57", "]"}]}], "Input",
