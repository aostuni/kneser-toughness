Cell[BoxData[{
 RowBox[{
  RowBox[{"g", "[", "c_", "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"lo", "=", 
       RowBox[{"c", "+", "1"}]}], ",", 
      RowBox[{"hi", "=", 
       RowBox[{"Ceiling", "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           FractionBox["10", "4"], "-", "1"}], ")"}], "c"}], "]"}]}], ",", 
      RowBox[{"m", "=", "0"}]}], "}"}], ",", 
    RowBox[{
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"lo", "<", "hi"}], ",", 
       RowBox[{
        RowBox[{"m", "=", 
         RowBox[{"Floor", "[", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"lo", "+", "hi"}], ")"}], "/", "2"}], "]"}]}], ";", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"l", "[", 
            RowBox[{"10", ",", "4", ",", "c", ",", "m"}], "]"}], ">", 
           RowBox[{"u", "[", 
            RowBox[{"10", ",", "4", ",", "c", ",", "m"}], "]"}]}], ",", 
          RowBox[{"lo", "=", 
           RowBox[{"m", "+", "1"}]}], ",", 
          RowBox[{"hi", "=", "m"}]}], "]"}]}]}], "]"}], ";", "lo"}]}], 
   "]"}]}], "\[IndentingNewLine]", 
